{
  "typescript.tsdk": "node_modules\\typescript\\lib",

  "i18n-ally.localesPaths": ["src/locales/lang"],
  "i18n-ally.enabledParsers": ["ts"],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.namespace": true,
  "i18n-ally.sourceLanguage": "en",
  "i18n-ally.displayLanguage": "zh",
  "i18n-ally.enabledFrameworks": ["vue", "react"],
  "i18n-ally.pathMatcher": "{locale}/{namespaces}.{ext}",

  // 控制相关文件嵌套展示
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*.ts": "$(capture).test.ts, $(capture).test.tsx",
    "*.tsx": "$(capture).test.ts, $(capture).test.tsx",
    "*.env": "$(capture).env.*",
    "CHANGELOG.md": "CHANGELOG*",
    "package.json": "package-lock.json,pnpm-lock.yaml,pnpm-workspace.yaml,LICENSE,.gitattributes,.gitignore,.gitpod.yml,CNAME,README*,.npmrc,.browserslistrc",
    ".eslintrc.js": ".eslintignore,.prettierignore,.stylelintignore,commitlint.config.js,.prettierrc.js,.stylelintrc.js",
    "babel.config.js": "postcss.config.js,stylelint.config.js,tailwind.config.js"
  },
  "terminal.integrated.scrollback": 10000
}
